{
  "name": "base-app",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@8.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tsc:watch": "tsc -p tsconfig.json --watch",
    "dev:generate-gql-schema": "dotenv -c development -- pnpm ./generate-gql-schema.js",
    "gql-codegen": "pnpm dev:generate-gql-schema && graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@auth0/auth0-react": "^2.1.0",
    "@sentry/react": "^7.52.1",
    "@stripe/react-stripe-js": "^2.1.0",
    "@stripe/stripe-js": "^1.53.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.13.1",
    "plaid": "^14.0.0",
    "plaid-threads": "^14.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plaid-link": "^3.4.0",
    "react-router-dom": "^6.11.1",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.0",
    "@graphql-codegen/introspection": "4.0.0",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "graphqurl": "^1.0.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "s3-spa-upload": "^2.1.5",
    "sass": "^1.62.1",
    "stripe": "^12.7.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.7",
    "vite-plugin-babel-macros": "^1.0.6",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
